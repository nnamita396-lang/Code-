#include<stdio.h>
int fibser(int n);
int fibser(int n){
if (n==0){
return 0;
}
if(n==1){
return 1;
}
else{
return fibser(n-1) + fibser(n-2);
}
}
int main(){
int n;
printf("enter n:");
scanf("%d",&n);
printf("Fibonacci series up to %d terms:\n", n);
for (int i = 0; i < n; i++) {
printf("%d ", fibser(i));
}
printf("\n");
}
